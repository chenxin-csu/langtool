<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
version="2.4"
xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee   http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">

	<display-name>Archetype Created Web Application</display-name>
	<context-param>
		<param-name>configInfoFilePath</param-name>
		<param-value>/WEB-INF/config-manager-info.xml</param-value>
	</context-param>
	<listener>
		<listener-class>com.twofishes.config.manager.ConfigWebListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.twofishes.config.patch.listener.LogUtilListener</listener-class>
	</listener>
	<listener>
		<listener-class>com.happyelements.mircale.web.listener.HttpRequesterListener</listener-class>
	</listener>
	<filter>
		<filter-name>MircaleWebFilter</filter-name>
		<filter-class>com.happyelements.mircale.web.filter.MircaleWebFilter</filter-class>
		<init-param>
			<param-name>encoding</param-name>
			<param-value>UTF-8</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>MircaleWebFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

